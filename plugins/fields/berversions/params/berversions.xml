<form>
  <fields name="params">
    <fieldset name="berversions_params" label="Bears Versions Parameters">
      <field name="berversions" type="subform"
             label="Bears Versions"
             description="Add your data here"
             multiple="true"
             layout="joomla.form.field.subform.repeatable-table">
        <form>
          <field name="url" type="url" label="Extension Release URL" description="Enter the release URL" default="" size="60" />
          <field name="icon" type="checkboxes" default="" label="Icons">
            <option value="full">Full Star</option>
            <option value="half">Half Star</option>
            <option value="archive">Archive</option>
            <option value="shield">Shield</option>
          </field>
        </form>
      </field>
    </fieldset>
  </fields>
</form>
